https://www.geeksforgeeks.org/problems/concatenate-integers/1?page=3&category=CPP&sortBy=submissions

Method-1:
#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;
    // Write Your Code to Concate and Print
    cout<<a<<b<<endl;

    return 0;
}

Method-2:
#include <iostream>
#include <string>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;

    string sa = to_string(a);
    string sb = to_string(b);
    string concatenated = sa + sb;

    int result = int(concatenated);

    cout << result << endl;

    return 0;
}
