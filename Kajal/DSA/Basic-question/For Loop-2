//User function Template for C++

void utility(string s){
    // length of string is given by s.length()
    // character at any index i is given by s[i]
    
    //Write your code here
    for(int i=0; i<s.length(); i++){
        if(i%2==0){
            cout<<s[i];
        }
    }
}
