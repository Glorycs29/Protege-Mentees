#include <iostream>
using namespace std;

int main() {
    // code here
    int a;
    cin>>a;
    
    int b;
    cin>>b;
    
    if(a>b){
        cout<<1<<endl;
    }
    else{
        cout<<0<<endl;
    }

    return 0;
}
