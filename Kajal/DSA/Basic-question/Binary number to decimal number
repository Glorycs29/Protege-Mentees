class Solution {
  public:
    int binaryToDecimal(string &b) {
        int result = 0;
        int power = 1;
        
        for(int i=b.size()-1; i>=0; i--){
            if(b[i]=='1'){
                result += power;
            }
            power *= 2;
        }
        return result;
    }
};
