#include <iostream>
#include <vector>
#include <sstream>
#include <algorithm>
using namespace std;

void performQueries(int T, vector<vector<string>> &allQueries) {
    for (int t = 0; t < T; ++t) {
        vector<int> A;
        for (int i = 0; i < allQueries[t].size(); ) {
            string q = allQueries[t][i];

            if (q == "a") {
                int x = stoi(allQueries[t][i + 1]);
                A.push_back(x);
                i += 2;
            }
            else if (q == "b") {
                sort(A.begin(), A.end());
                i++;
            }
            else if (q == "c") {
                reverse(A.begin(), A.end());
                i++;
            }
            else if (q == "d") {
                cout << A.size() << " ";
                i++;
            }
            else if (q == "e") {
                for (int val : A) cout << val << " ";
                i++;
            }
            else if (q == "f") {
                sort(A.rbegin(), A.rend()); // descending sort
                i++;
            }
        }
        cout << endl;
    }
}
