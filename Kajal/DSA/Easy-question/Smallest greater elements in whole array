#include <iostream>
#include <vector>
#include <set>
using namespace std;

vector<int> greaterElement(int arr[], int n) {
    set<int> s;
    vector<int> ans(n, -10000000);

    for (int i = n - 1; i >= 0; --i) {
        auto it = s.upper_bound(arr[i]);  // Get next greater element
        if (it != s.end())
            ans[i] = *it;

        s.insert(arr[i]);  // Add current element to set
    }

    return ans;
}
