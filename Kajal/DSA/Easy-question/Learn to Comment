#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;
    // Comment the part that outputs b
    cout << a << " ";
//    cout << b << " ";
    cout << c << " ";
    cout << endl;

    return 0;
}
